{
    "name": "{{ packageName }}",
    "description": "{{ packageDesc }}",
    "keywords": [
{% for key, keyword in packageKeywords %}
    {% if loop.last %}
"{{ keyword|trim|capitalize }}"
    {% else %}
"{{ keyword|trim|capitalize }}",
    {% endif %}
{% endfor %}
],
{% if homepage %}
    "homepage": "{{ homepage }}",
{% endif %}
    "license": "{{ license }}",
    "type": "{{ packageType }}",
    "authors": [
        {
            "name": "{{ author }}",
            "email": "{{ email }}"
        }
    ],
    "require": {

    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            {{ psr4|raw }}
        }
    },
    "minimum-stability": "{{ minStability }}",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}