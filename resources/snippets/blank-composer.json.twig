{
    "name": "{{ packageName }}",
    "description": "{{ packageDesc }}",
    "keywords": [
{{ include('snippets/partials/composer-keywords.twig') }}
    ],
{% if homepage %}
    "homepage": "{{ homepage }}",
{% endif %}
    "license": "{{ license }}",
    "type": "{{ packageType }}",
    "authors": [
        {
            "name": "{{ author }}",
            "email": "{{ email }}"
        }
    ],
    "require": {

    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            {{ psr4|raw }}
        }
    },
    "minimum-stability": "{{ minStability }}",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}