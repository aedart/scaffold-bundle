{
  "name": "aedart/scaffold-bundle",
  "description": "Collection of scaffolds",
  "keywords": [
    "Scaffolds",
    "Templates",
    "Snippets"
  ],
  "homepage": "https://github.com/aedart/scaffold-bundle",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "aedart/license": "1.*",
    "aedart/license-file-manager": "~1.5",
    "illuminate/support": "5.4.*"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\Scaffold\\Bundle\\": "src/"
    },
    "files": [
      "helpers/helpers.php"
    ]
  },
  "suggest": {
    "aedart/scaffold": "So you can install and make use of these scaffolds"
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
